{
  "compilerOptions": {
    "target": "es2020",
     "lib": [
       "ES6"
     ],"module": "esnext",
    "rootDir": "./",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "allowJs": true,
    "sourceMap": true,
    "outDir": "./dist","allowSyntheticDefaultImports": true,
    "esModuleInterop": true,"forceConsistentCasingInFileNames": true,"strict": true,
    "noImplicitAny": true,"skipLibCheck": true
  },
  "include": ["./**/*"],
  "exclude": ["client", "node_modules", "dist"],
  "lib": ["esnext"],
  "ts-node": {
    "esm": true
  }
}
